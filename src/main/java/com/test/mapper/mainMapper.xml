<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.mapper.mainMapper">
  
	<select id ="list" resultType ="com.test.mapper.guest">
	    select * from test1
	</select>
	
	<select id="memberLogin" parameterType ="com.test.mapper.guest" resultType="com.test.mapper.guest">
		SELECT user_id , user_name,user_phone,user_expire_date
		FROM users
		WHERE user_id = #{user_id}
		AND user_gym = #{user_gym}
	</select>
</mapper>